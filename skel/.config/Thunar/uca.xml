	<description>edit as root</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>application-x-root</icon>
	<name>Ownership to user</name>
	<unique-id>1599628983536855-4</unique-id>
